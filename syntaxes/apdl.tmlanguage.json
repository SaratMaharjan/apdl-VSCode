{

	"name": "apdl",

	"fileTypes": [
		"mac",
		"ans",
		"in",
		"out",
		"csv"
	],

	"foldingStartMarker": "^\\s*\\*(?i)(if|do|dowhile|!\\sende).*\\s*$",
	"foldingStopMarker": "^\\s*\\*(?i)(endif|enddo|!\\sanfang).*\\s*$",

	"patterns": [{
			"include": "#general"
		},

		{
			"include": "#variable"
		},

		{
			"include": "#comment_line"
		},

		{
			"include": "#string_single"
		},

		{
			"include": "#string_double"
		},

		{
			"include": "#keywords1"
		},

		{
			"include": "#keywords2"
		},
		{
			"include": "#keywords3"
		},
		{
			"include": "#elementTypes"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#operators"
		},
		{
			"include": "#special"
		}

	],

	"repository": {

		"general": {
			"comment": "all items when not specified",
			"patterns": [{
					"include": "#variable"
				},
				{
					"include": "#comment_line"
				}
			]
		},

		"variable": {
			"match": "(\\$|\\-\\-)[A-Za-z0-9_-]+\\b",
			"name": "variable.apdl"
		},

		"comment_line": {
			"name": "comment.line.apdl",
			"begin": "!",
			"end": "$",
			"patterns": [{
				"include": "#special"
			}]
		},

		"string_single": {
			"name": "string.quoted.single.apdl",
			"begin": "'",
			"end": "'"
		},

		"string_double": {
			"name": "string.quoted.double.apdl",
			"begin": "\"",
			"end": "\""
		},

